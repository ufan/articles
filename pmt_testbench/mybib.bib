	keywords = {Astroparticle physics, Cosmic rays, Photodetectors, Pierre Auger, PMTs, PMT testing, Water Cherenkov},
	pages = {453--466},
	file = {ScienceDirect Full Text PDF:/home/ufan/Documents/zotero_lib_linux/storage/U8A8PU4I/Barnhill et al. - 2008 - Testing of photomultiplier tubes for use in the su.pdf:application/pdf;ScienceDirect Snapshot:/home/ufan/Documents/zotero_lib_linux/storage/CMSCA8K8/S0168900208001563.html:text/html}
}

@article{akgun_complete_2005,
