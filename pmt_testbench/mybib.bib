	volume = {550},
	issn = {0168-9002},
	url = {http://www.sciencedirect.com/science/article/pii/S0168900205012349},
	doi = {10.1016/j.nima.2005.03.171},
	abstract = {Approximately 2000 PMTs will be used to detect the Cherenkov light generated in quartz fibers embedded in the CMS-HF Forward Calorimeter. The Hamamatsu R7525HA PMT was chosen for this purpose. We measured the transit time, transit time spread, pulse width, rise time, anode dark current, and relative gain for each tube in the test station at the University of Iowa. Life-time, gain versus high voltage, and single photoelectron spectrum measurements were also done on a small sample of PMTs. All the tubes were tested to verify that they conform to the HF requirements.},
	number = {1–2},
	urldate = {2015-05-31},
	journal = {Nuclear Instruments and Methods in Physics Research Section A: Accelerators, Spectrometers, Detectors and Associated Equipment},
	author = {Akgun, U. and Ayan, A. S. and Bruecken, P. and Duru, F. and Gülmez, E. and Mestvirishvilli, A. and Miller, M. and Olson, J. and Onel, Y. and Schmidt, I.},
	month = sep,
	year = {2005},
	keywords = {CMS, HF-calorimeter, PMT},
	pages = {145--156},
	file = {ScienceDirect Full Text PDF:/home/ufan/Documents/zotero_lib_linux/storage/MNK3VUC3/Akgun et al. - 2005 - Complete tests of 2000 Hamamatsu R7525HA phototube.pdf:application/pdf;ScienceDirect Snapshot:/home/ufan/Documents/zotero_lib_linux/storage/ZPFBEB3X/S0168900205012349.html:text/html}
}